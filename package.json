{
  "name": "react-starterkit",
  "version": "0.0.0",
  "private": true,
  "homepage": ".",
  "dependencies": {
    "@material-ui/core": "3.1.2",
    "@material-ui/icons": "3.0.1",
    "@types/react-router-dom": "4.3.2",
    "chalk": "1.1.3",
    "dayjs": "1.7.7",
    "dompurify": "1.0.7",
    "enzyme": "3.3.0",
    "enzyme-adapter-react-16": "1.7.1",
    "firebase": "5.9.4",
    "fs-extra": "3.0.1",
    "intl": "1.2.5",
    "is-mobile": "1.0.0",
    "jest": "23.6.0",
    "js-base64": "2.4.5",
    "jss": "9.8.7",
    "localforage": "1.7.2",
    "lodash": "4.17.11",
    "object-assign": "4.1.1",
    "promise": "8.0.1",
    "raf": "3.4.0",
    "react": "16.5.0",
    "react-dom": "16.5.0",
    "react-firebaseui": "3.1.2",
    "react-intl": "2.4.0",
    "react-jss": "8.6.1",
    "react-redux": "5.1.0",
    "react-router-dom": "5.0.0",
    "recompose": "0.30.0",
    "redux": "4.0.0",
    "redux-logger": "3.0.6",
    "redux-mock-store": "1.5.3",
    "redux-observable": "1.0.0",
    "redux-persist": "5.10.0",
    "resolve": "1.6.0",
    "rxjs": "6.2.2",
    "typeface-roboto": "0.0.54",
    "typesafe-actions": "3.2.1",
    "uuid": "3.2.1",
    "whatwg-fetch": "2.0.3"
  },
  "scripts": {
    "start": "node scripts/start.js",
    "build": "node scripts/build.js",
    "test": "cross-env TZ=UTC node scripts/test.js --env=jsdom",
    "test-coverage": "cross-env TZ=UTC node scripts/test.js --env=jsdom --coverage --reporters=jest-junit --reporters=default --coverageReporters=cobertura --coverageReporters=html",
    "ci:build": "cross-env NODE_ENV=production CI=true GENERATE_SOURCEMAP=false npm-run-all build test-coverage",
    "ci:publish": "npm-run-all ci:build --"
  },
  "devDependencies": {
    "@types/dompurify": "0.0.31",
    "@types/enzyme": "3.1.11",
    "@types/enzyme-adapter-react-16": "1.0.3",
    "@types/intl": "1.2.0",
    "@types/jest": "23.3.3",
    "@types/js-base64": "2.3.1",
    "@types/lodash": "4.14.118",
    "@types/node": "10.11.4",
    "@types/react": "16.4.15",
    "@types/react-dom": "16.0.8",
    "@types/react-intl": "2.3.8",
    "@types/react-redux": "6.0.9",
    "@types/recompose": "0.27.0",
    "@types/redux-devtools-extension": "2.13.2",
    "@types/redux-logger": "3.0.6",
    "@types/redux-mock-store": "1.0.0",
    "@types/uuid": "3.4.3",
    "archiver": "3.0.0",
    "autoprefixer": "7.1.6",
    "babel-jest": "23.6.0",
    "babel-loader": "8.0.4",
    "case-sensitive-paths-webpack-plugin": "2.1.2",
    "cross-env": "5.2.0",
    "css-loader": "1.0.1",
    "dotenv": "4.0.0",
    "dotenv-expand": "4.2.0",
    "file-loader": "2.0.0",
    "fork-ts-checker-webpack-plugin": "0.4.14",
    "glob": "7.1.3",
    "hard-source-webpack-plugin": "0.12.0",
    "html-webpack-plugin": "4.0.0-beta.2",
    "jest-junit": "5.2.0",
    "mini-css-extract-plugin": "0.4.4",
    "npm-run-all": "4.1.5",
    "postcss-flexbugs-fixes": "3.2.0",
    "postcss-loader": "3.0.0",
    "react-dev-utils": "6.1.1",
    "redux-devtools-extension": "2.13.5",
    "rimraf": "2.6.2",
    "source-map-loader": "0.2.4",
    "style-loader": "0.23.1",
    "sw-precache-webpack-plugin": "0.11.5",
    "thread-loader": "1.2.0",
    "ts-jest": "23.10.4",
    "ts-loader": "5.3.0",
    "tsconfig-paths-webpack-plugin": "3.2.0",
    "tslint": "5.11.0",
    "tslint-config-prettier": "1.15.0",
    "tslint-microsoft-contrib": "5.2.1",
    "tslint-react": "3.6.0",
    "typescript": "3.1.1",
    "uglifyjs-webpack-plugin": "2.0.1",
    "url-loader": "1.1.2",
    "webpack": "4.19.0",
    "webpack-dev-server": "3.1.10",
    "webpack-manifest-plugin": "2.0.4",
    "yargs": "12.0.4"
  },
  "jest": {
    "collectCoverageFrom": [
      "src/**/*.{js,jsx,ts,tsx}",
      "!**/*.d.ts"
    ],
    "setupFiles": [
      "<rootDir>/config/polyfills.js"
    ],
    "setupTestFrameworkScriptFile": "<rootDir>/src/setupTests.ts",
    "testMatch": [
      "<rootDir>/src/**/__tests__/**/*.(j|t)s?(x)",
      "<rootDir>/src/**/?(*.)(spec|test).(j|t)s?(x)"
    ],
    "testEnvironment": "node",
    "testURL": "http://localhost",
    "transform": {
      "^.+\\.(js|jsx|mjs)$": "<rootDir>/node_modules/babel-jest",
      "^.+\\.tsx?$": "<rootDir>/node_modules/ts-jest",
      "^.+\\.css$": "<rootDir>/config/jest/cssTransform.js",
      "^(?!.*\\.(js|jsx|mjs|css|json)$)": "<rootDir>/config/jest/fileTransform.js"
    },
    "transformIgnorePatterns": [
      "[/\\\\]node_modules[/\\\\].+\\.(js|jsx|mjs|ts|tsx)$"
    ],
    "moduleNameMapper": {
      "@components/(.*)": "<rootDir>/src/components/$1",
      "@state/(.*)": "<rootDir>/src/state/$1",
      "@model/(.*)": "<rootDir>/src/model/$1",
      "@core/(.*)": "<rootDir>/src/core/$1"
    },
    "moduleFileExtensions": [
      "web.ts",
      "ts",
      "web.tsx",
      "tsx",
      "web.js",
      "js",
      "web.jsx",
      "jsx",
      "json",
      "node",
      "mjs"
    ],
    "globals": {
      "ts-jest": {
        "tsConfig": "<rootDir>/tsconfig.test.json"
      }
    }
  },
  "jest-junit": {
    "outputDirectory": "coverage"
  }
}
